generator client {
  provider = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model News {
  id          Int      @id @default(autoincrement())
  title       String
  summary     String
  content     String
  image       String?
  video       String?
  publishedAt DateTime @default(now())
} 